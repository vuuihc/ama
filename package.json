{
  "name": "7dyk",
  "version": "0.0.0",
  "description": "7dyk frontend",
  "scripts": {
    "build": "set NODE_ENV=development &webpack --progress --colors --watch -p",
    "watch": "set NODE_ENV=development & webpack-dev-server --hot --inline --progress --colors",
    "dist": "set NODE_ENV=production & webpack --progress --colors -p",
    "commit": "git add dist/mobile.js dist/mobile.js.map src/* & git commit -am 'update' & git push"
  },
  "repository": {
    "type": "git"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ]
  },
  "dependencies": {
    "babel-polyfill": "^6.9.1",
    "isomorphic-fetch": "^2.2.1",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.0",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "sass-loader": "^4.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "classnames": "^2.2.5",
    "css-loader": "^0.23.1",
    "file-loader": "^0.9.0",
    "node-sass": "^3.8.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1"
  }
}
